{
  "printWidth": 100,
  "singleQuote": true,
  "useTabs": false,
  "semi": false,
  "tabWidth": 2,
  "trailingComma": "none",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "importOrder": ["^@heyform-inc/", "^@(decorator|graphql|guard|dto|interceptor|middleware|config|environments|controller|model|resolver|service|schedule|utils)", "^[.+/]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderGroupNamespaceSpecifiers": false,
  "importOrderParserPlugins": [
    "typescript",
    "decorators-legacy"
  ]
}
